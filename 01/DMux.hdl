/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    And(a = in, b = sel, out = b);
    Not(in = sel , out = Notsel);
    And(a = Notsel, b = in, out = a);
}
